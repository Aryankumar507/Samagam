<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#373434"
    >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TESLA"
        android:textSize="30sp"
        android:gravity="center"
        android:textStyle="bold"
        android:letterSpacing="0.15"
        android:padding="15dp"
        android:textColor="#CE6ADF"
        />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_160sdp"
        android:layout_marginHorizontal="15dp"
        app:cardCornerRadius="@dimen/_35sdp"
        android:elevation="20dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_gravity="bottom"
            android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_130sdp"
                app:cardBackgroundColor="#d1c1ff"
                android:background="#d1c1ff"
                android:layout_gravity="bottom"
                android:layout_marginEnd="@dimen/_10sdp"
                app:cardCornerRadius="@dimen/_28sdp"
                android:elevation="@dimen/_15sdp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_marginStart="15dp"
                        android:layout_marginEnd="10dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Office bearer 2024"
                            android:textStyle="bold"
                            android:layout_marginTop="@dimen/_14sdp"
                            android:textSize="25dp"
                            android:textColor="@color/black"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                app:cardCornerRadius="15dp"
                                android:layout_marginEnd="6dp"
                                android:layout_marginBottom="10dp"
                                android:layout_weight="1">
                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@color/black"/>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                app:cardCornerRadius="15dp"
                                android:layout_marginEnd="6dp"
                                android:layout_marginBottom="10dp"
                                android:layout_weight="1">
                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@color/black"/>

                            </androidx.cardview.widget.CardView>
                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                app:cardCornerRadius="15dp"
                                android:layout_marginEnd="6dp"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/outdent"
                                android:layout_weight="1">
                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@color/black"/>

                            </androidx.cardview.widget.CardView>
                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                app:cardCornerRadius="15dp"
                                android:layout_marginEnd="6dp"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/outdent"
                                android:layout_weight="1">
                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@color/black"/>

                            </androidx.cardview.widget.CardView>


                        </LinearLayout>




                    </LinearLayout>
                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="#fed8ea"
                        android:elevation="12dp"
                        android:background="@drawable/shadow_black"
                        android:layout_weight="0.17">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/outdent"/>

                    </androidx.cardview.widget.CardView>


                </LinearLayout>


            </androidx.cardview.widget.CardView>

        </RelativeLayout>


    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        app:cardCornerRadius="35dp"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_marginHorizontal="@dimen/_10sdp"
        android:elevation="10dp"
        >
        <androidx.cardview.widget.CardView
            android:layout_marginTop="50dp"
            app:cardCornerRadius="35dp"
            app:cardElevation="35dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="305dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/contact_details_purple"
                    android:gravity="center|top"
                    android:orientation="vertical"
                    >


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Office bearer 2024"
                        android:textStyle="bold"
                        android:textSize="25dp"
                        android:textColor="@color/black"/>
                    <LinearLayout
                        android:layout_width="305dp"
                        android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="18dp"
                            android:layout_marginTop="15dp"
                            >

                            <ImageView
                                android:id="@+id/ivOB1"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView

                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="15dp"
                            >

                            <ImageView
                                android:id="@+id/ivOB2"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="15dp"
                            >

                            <ImageView
                                android:id="@+id/ivOB3"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="15dp"
                            >

                            <ImageView
                                android:id="@+id/ivOB4"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/contact_details_pink"
                    >

                    <ImageView
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:src="@drawable/outdent"
                        android:layout_gravity="center"
                        />
                </LinearLayout>



            </LinearLayout>
        </androidx.cardview.widget.CardView>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:cardCornerRadius="35dp"
        app:cardElevation="9dp"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:background="@drawable/contact_details_whitebackground"
        android:layout_marginHorizontal="@dimen/_10sdp"
        android:layout_marginTop="25dp"
        >
        <androidx.cardview.widget.CardView
            android:layout_marginTop="50dp"
            app:cardCornerRadius="35dp"
            app:cardElevation="7dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="305dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/contact_details_purple"
                    android:gravity="center|top"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Club Events"
                        android:textStyle="bold"
                        android:textSize="25dp"
                        android:textColor="@color/black"/>
                    <LinearLayout
                        android:layout_width="305dp"
                        android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="120dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="15dp"
                            >
                            <ImageView
                                android:id="@+id/ivCE1"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="5dp"
                            android:layout_width="120dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="15dp">
                            <ImageView
                                android:id="@+id/ivCE2"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/clubEvent"
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/contact_details_pink">

                    <ImageView
                        android:layout_width="70dp"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        android:src="@drawable/outdent" />
                </LinearLayout>


            </LinearLayout>
        </androidx.cardview.widget.CardView>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:cardCornerRadius="35dp"
        app:cardElevation="20dp"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:background="@drawable/contact_details_whitebackground"
        android:layout_marginHorizontal="@dimen/_10sdp"
        android:layout_marginTop="25dp"
        >
        <androidx.cardview.widget.CardView
            android:layout_marginTop="50dp"
            app:cardCornerRadius="35dp"
            app:cardElevation="35dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="305dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/contact_details_purple"
                    android:gravity="center|top"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Alumini section"
                        android:textStyle="bold"
                        android:textSize="25dp"
                        android:textColor="@color/black"/>
                    <LinearLayout
                        android:layout_width="305dp"
                        android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="15dp"
                            >
                            <ImageView
                                android:id="@+id/ivAS1"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="15dp">
                            <ImageView
                                android:id="@+id/ivAS2"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="15dp"
                            >
                            <ImageView
                                android:id="@+id/ivAS3"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            app:cardCornerRadius="@dimen/_5sdp"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="15dp">
                            <ImageView
                                android:id="@+id/ivAS4"
                                android:scaleType="centerCrop"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/contact_details_pink">
                    <ImageView
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:src="@drawable/outdent"
                        android:layout_gravity="center"
                        />
                </LinearLayout>


            </LinearLayout>
        </androidx.cardview.widget.CardView>

    </androidx.cardview.widget.CardView>

</LinearLayout>
